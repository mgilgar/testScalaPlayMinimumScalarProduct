# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

# Minimun Scalar Product
GET		/minimumScalarProduct/:input1/:input2		controllers.MinimumScalarProductController.minimumScalarProductUsingActors(input1: String, input2: String)


# Web socket
GET		/echo						controllers.Echo.processEcho
GET		/echo.html					controllers.Echo.viewEchoPage